{
  "manifest_version": 3,
  "name": "Graphite to GitHub PR Redirector",
  "version": "1.0",
  "description": "Automatically redirects Graphite PR links to GitHub",
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://app.graphite.dev/*",
    "*://app.graphite.com/*"
  ],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
